  "scripts": {
    "dev:wds": "cross-env NODE_ENV=development IS_SSR=0 IS_WDS=1 webpack-dev-server --config ./webpack/wds.babel.js",
    "dev:ssroff": "npm run clean && cross-env NODE_ENV=development IS_SSR=0 nodemon --watch server --exec babel-node --inspect --extensions '.js,.jsx' src/server/index.js",
    "dev:ssr": "npm run clean && cross-env NODE_ENV=development IS_SSR=0 nodemon --watch server --exec babel-node --inspect --extensions '.js,.jsx' src/server/index.js",
    "prod": "cross-env NODE_ENV=production IS_SSR=1 node ./build/bundle.server.js",
    "prod:debug": "npm run build && cross-env NODE_ENV=development IS_SSR=1 node --inspect ./build/bundle.server.js",
    "prod:build": "npm run build && cross-env NODE_ENV=production IS_SSR=1 node ./build/bundle.server.js",
    "prod:ssroff": "cross-env NODE_ENV=production IS_SSR=0 node ./build/bundle.server.js",
    "build": "npm run clean && npm-run-all -p build:client build:server",
    "build:client": "cross-env NODE_ENV=production IS_SSR=0 webpack --mode development --config ./webpack/prod.babel.js",
    "build:dev": "cross-env NODE_ENV=development IS_SSR=0 webpack --mode development --config ./webpack/wds.babel.js",
    "build:server": "cross-env NODE_ENV=development IS_SSR=0 webpack --mode development --config ./webpack/server.babel.js",
    "build:client_nossr": "cross-env NODE_ENV=production IS_SSR=0 webpack --mode production --config ./webpack/prod.babel.js",
    "build:server_nossr": "cross-env NODE_ENV=development IS_SSR=0 webpack --mode development --config ./webpack/server.babel.js",
    "build:ssroff": "npm run clean && npm-run-all -p build:client_nossr build:server_nossr",
    "start": "cross-env NODE_PATH=. nodemon --exec babel-node -r src/server/dotenv.config.js src/server/index.js",
    "test": "jest --config jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage",
    "test:u": "npm test -- -u",
    "fix-memory-limit": "cross-env LIMIT=16000 increase-memory-limit",
    "clean": "rm -rf build/*",
    "todos": "leasot --exit-nicely ./src/**/*.js ./src/**/*.jsx"
  },
